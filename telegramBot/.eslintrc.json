{
  "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true
  },
  "extends": [
    "airbnb-base",
    "plugin:prettier/recommended"
  ],
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaVersion": 2020
  },
  "plugins": ["prettier"],
  "rules": {
    "no-console": "off",
    "consistent-return": "off",
    "import/prefer-default-export": "off"
  }
}